    "@types/node": "^20.17.55",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "resolve-tspaths": "^0.8.23",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  },
  "license": "MIT"
}
